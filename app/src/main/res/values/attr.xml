<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FreeTabLayout">
        <attr name="tabLayoutIndicatorColor" format="color" />
        <attr name="tabLayoutIndicatorWidth" format="dimension" />
        <attr name="tabLayoutIndicatorHeight" format="dimension" />
        <attr name="tabLayoutIndicatorRadius" format="dimension" />
        <attr name="tabLayoutContentStart" format="dimension" />
        <attr name="tabLayoutBackground" format="reference" />
        <attr name="tabLayoutMode">
            <enum name="scrollable" value="0" />
            <enum name="fixed" value="1" />
        </attr>
        <attr name="tabLayoutGravity">
            <enum name="fill" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="tabLayoutMinWidth" format="dimension" />
        <attr name="tabLayoutMaxWidth" format="dimension" />
        <attr name="tabLayoutTextAppearance" format="reference" />
        <attr name="tabLayoutTextColor" format="color" />
        <attr name="tabLayoutSelectedTextColor" format="color" />
        <attr name="tabLayoutPaddingStart" format="dimension" />
        <attr name="tabLayoutPaddingTop" format="dimension" />
        <attr name="tabLayoutPaddingEnd" format="dimension" />
        <attr name="tabLayoutPaddingBottom" format="dimension" />
        <attr name="tabLayoutPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FreeTabItem">
        <attr name="android:text" />
        <attr name="android:icon" />
        <attr name="android:layout" />
    </declare-styleable>
</resources>